{
    "manifest_version": 2,
    "name": "SaveOverflow",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [ "https://stackoverflow.com/*","http://stackoverflow.com/*" ],
            "js": ["./lib/jquery-3.0.0.min.js", "content.js"]
            }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "./assets/images/icon.png"
    }
}
